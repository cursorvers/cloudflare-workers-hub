# Workers Hub API Keys
# Copy to .env and fill in values

# Queue API (for AI Assistant Daemon, Local Agent)
QUEUE_API_KEY=

# Memory API (for conversation history)
MEMORY_API_KEY=

# Admin API (for management operations)
ADMIN_API_KEY=

# Cockpit WebSocket (for PWA real-time)
# Same as QUEUE_API_KEY by default

# VAPID Keys (for Web Push notifications)
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=

# JWT Keys (for RBAC - optional, uses HS256 with QUEUE_API_KEY if not set)
# JWT_PRIVATE_KEY=
# JWT_PUBLIC_KEY=
